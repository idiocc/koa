{
  "name": "@goa/koa",
  "version": "2.0.0",
  "description": "The Koa2 Fork Optimized With Closure Compiler That Has Just 1 Dependency (mime-db).",
  "main": "compile/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "ALAMODE_ENV=test-build yarn t test.js test/spec",
    "compile": "depack src/depack -o compile/koa.js -c -a -p --source_map_include_content",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "build": "yarn-s d b doc",
    "d": "yarn-s template dv typework",
    "template": "typal compile/template.js -T compile/index.js -t node_modules/@goa/goa/types",
    "dv": "typal types/vendor/*",
    "typework": "typework types/config.json",
    "e": "alanode"
  },
  "files": [
    "compile",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/koa.git"
  },
  "keywords": [
    "koa",
    "goa",
    "idio",
    "gcc",
    "closure",
    "closure compiler",
    "web",
    "server",
    "http"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/idiocc/koa/issues"
  },
  "homepage": "https://www.idio.cc",
  "devDependencies": {
    "@contexts/http": "^1.6.1",
    "@externs/goa": "^0.1.3",
    "@goa/goa": "^1.4.3",
    "@typedefs/goa": "^0.0.11",
    "alamode": "^3.3.0",
    "depack": "^1.0.1",
    "documentary": "^1.34.9",
    "eslint-config-artdeco": "1.0.1",
    "typework": "^0.1.1",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  },
  "engines": {
    "node": ">=7.7"
  },
  "dependencies": {
    "mime-db": "^1.42.0"
  }
}
